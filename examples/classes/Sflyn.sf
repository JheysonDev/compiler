class Sflyn {

  private readonly lang: string;

  constructor(
    private year: number,
  ) {
    this.lang = 'Sflyn';
  }

  public getLang(): string {
    return this.lang;
  }

  getYear(): number {
    return this.year;
  }

}

let sflyn = new Sflyn(2020);

log->write(sflyn->getLang());
log->write(sflyn->getYear());
